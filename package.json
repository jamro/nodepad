{
  "name": "@jamropl/nodepad",
  "version": "1.6.0",
  "description": "a simple tool to manage your NodeJs apps through REST API or UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamro/nodepad"
  },
  "scripts": {
    "commit": "cz",
    "start": "node ./bin/nodepad",
    "watch": "concurrently --kill-others 'npm:watch-server' 'npm:watch-ui'",
    "watch-server": "nodemon --config run.nodemon.json --exec \"npm run start\"",
    "watch-ui": "webpack --watch --progress --config webpack.config.js --mode=development",
    "build": "webpack --config webpack.config.js --mode=production",
    "lint": "eslint \"**/*.js\" --ignore-pattern node_modules --ignore-pattern tmp --ignore-pattern projects/ --fix",
    "test": "mocha --recursive --require @babel/register --exclude test/proxy-server/**/*.js test",
    "test-watch": "nodemon --config test.nodemon.json --exec \"npm run test\"",
    "all": "npm run lint && npm run build && npm run test",
    "pm2": "pm2"
  },
  "dependencies": {
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "busboy": "^1.6.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.2",
    "ejs": "~3.1.6",
    "express": "~4.17.1",
    "express-openapi": "^9.3.0",
    "express-winston": "^4.2.0",
    "extract-zip": "^2.0.1",
    "http-errors": "~1.8.0",
    "http-proxy-middleware": "^2.0.1",
    "morgan": "~1.10.0",
    "ncp": "^2.0.0",
    "node-http-proxy": "^0.2.4",
    "pm2": "^5.1.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "read-last-lines": "^1.8.0",
    "semantic-ui-react": "^2.0.4",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "swagger-ui-express": "^4.1.6",
    "websocket": "^1.0.34",
    "winston": "^3.3.3",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-import-ignore": "^1.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "form-data": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^18.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mocha": "^9.1.3",
    "mock-req": "^0.2.0",
    "nodemon": "^2.0.14",
    "sinon": "^11.1.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "why-is-node-running": "^2.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
