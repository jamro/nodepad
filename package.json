{
  "name": "nodepad",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "cz",
    "start": "node ./bin/www",
    "watch": "concurrently --kill-others 'npm:watch-server' 'npm:watch-ui'",
    "watch-server": "nodemon --config run.nodemon.json --exec \"npm run start\"",
    "watch-ui": "webpack --watch --progress --config webpack.config.js --mode=development",
    "build": "webpack --config webpack.config.js --mode=production",
    "lint": "eslint \"**/*.js\" --ignore-pattern node_modules --ignore-pattern tmp --ignore-pattern projects/ --fix",
    "test": "mocha --recursive --require @babel/register test",
    "test-watch": "nodemon --config test.nodemon.json --exec \"npm run test\"",
    "all": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@babel/register": "^7.13.16",
    "@babel/runtime": "^7.14.0",
    "adm-zip": "^0.5.5",
    "busboy": "^0.3.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-openapi": "^9.0.0",
    "express-winston": "^4.1.0",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^2.0.0",
    "morgan": "~1.9.1",
    "node-http-proxy": "^0.1.5",
    "pm2": "^4.5.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "read-last-lines": "^1.8.0",
    "semantic-ui-react": "^2.0.3",
    "swagger-ui-express": "^4.1.6",
    "websocket": "^1.0.34",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.24.0",
    "form-data": "^2.3.3",
    "html-webpack-plugin": "^5.3.1",
    "jsdom": "^16.6.0",
    "mocha": "^8.4.0",
    "mock-req": "^0.2.0",
    "nodemon": "^2.0.7",
    "sinon": "^10.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "why-is-node-running": "^2.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
